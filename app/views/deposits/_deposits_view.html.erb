<div class="deposits__list">
  <div class="deposits__list__id">
    ID:<span><%= deposit.id %></span>
  </div>
  <div class="deposits__list__name">
    商品名：<span><%= deposit.name %></span>
  </div>
  <div class="deposits__list__assesment_price">
    査定額：<span>¥<%= deposit.assesment_price %></span>
  </div>
  <div class="deposits__list__user">
    所持ユーザー：<span><%= deposit.user.name %>様</span>
  </div>
  <div class="deposits__list__warehouse_id">
    保管倉庫：<span><%= deposit.warehouse_id %>番</span>
  </div>
  <div class="deposits__list__brand">
    ブランド名：<span><%= deposit.brand %></span>
  </div>
  <div class="deposits__list__size">
    サイズ：<span><%= deposit.size %>
  </div>
</div>
