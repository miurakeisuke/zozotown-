<div class=wrapper>
  <div class=center_content>
    <div class=breadcrumbs>
      <%= render_breadcrumbs separator: ' ＞ ' %>
    </div>
    <div class=index_header>
      <i class="fa fa-tag fa-2x"></i>
      <h1>
        ブランド一覧
      </h1>
      <div class="brand_content_header">
        <ul class="brand_main_tab">
          <li id="tab_all" class="brand_selected"><a class="selected">すべて</a></li>
          <li id="tab_men" class="brand_select_list"><a href="/ranking/all-sales-men.html" class="brand_none_selected">メンズ</a></li>
          <li id="tab_women" class="brand_select_list"><a href="/ranking/all-sales-women.html" class="brand_none_selected">レディース</a></li>
          <li id="tab_kids" class="brand_select_list"><a href="/ranking/all-sales-kids.html" class="brand_none_selected">キッズ</a>
        </ul>
        <ul class="zozo_tab">
          <li class="shop_zozotown">
            ZOZOTOWN |
          </li>
          <li class="shop_zozotown">
            <%= link_to 'ZOZOUSED', "https://www.google.co.jp/", class: "zozoused" %>
          </li>
        </ul>
      </div>
    </div>
    <div class="search_zone">
      <div class=search_area>
        <p>
          <input class="search_query" name="keyword" placeholder=" &#xF002;お探しのブランドは何ですか？" type="text" value="">
          </input>
        </p>
      </div>
      <div class=initial_zone>
        <ul>
          <li><a href="#initial_a">A</a></li>
          <!-- スクロールできるくらいコンテンツが増えると、ページ内リンクするようになります。 -->
          <li><a href="#initial_b">B</a></li>
          <li>C</li>
          <li>D</li>
          <li>E</li>
          <li>F</li>
          <li>G</li>
          <li>H</li>
          <li>I</li>
          <li>J</li>
          <li>K</li>
          <li>L</li>
          <li>M</li>
          <li>N</li>
          <li>O</li>
          <li>P</li>
          <li>Q</li>
          <li>R</li>
          <li>S</li>
          <li>T</li>
          <li>U</li>
          <li>V</li>
          <li>W</li>
          <li>X</li>
          <li>Y</li>
          <li>Z</li>
          <li>その他</li>
        </ul>
      </div>
    </div>
    <div class="brand_result_wrapper">
      <dl class="brand_description_list">
        <dt id="initial_a" class="brand_description_term">A</dt>
        <dd>
          <%= link_to 'AAA', "https://www.google.co.jp/", class: "brand_list_company" %>
          <span class="brand_company_count">(4)
          </span>
        </dd>
        <dd>
          <%= link_to 'ABC', "https://www.google.co.jp/", class: "brand_list_company" %>
          <span class="brand_company_count">(4)
          </span>
        </dd>
        <dd>
          <%= link_to 'ACB', "https://www.google.co.jp/", class: "brand_list_company" %>
          <span class="brand_company_count">(4)
          </span>
        </dd>
        <dd>
          <%= link_to 'ACC', "https://www.google.co.jp/", class: "brand_list_company" %>
          <span class="brand_company_count">(4)
          </span>
        </dd>
      </dl>
    </div>
    <div class="brand_result_wrapper">
      <dl class="brand_description_list">
        <dt id="initial_b" class="brand_description_term">B</dt>
        <dd>
          <%= link_to 'BAA', "https://www.google.co.jp/", class: "brand_list_company" %>
