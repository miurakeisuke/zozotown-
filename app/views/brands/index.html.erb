<div class=wrapper>
  <div class=center_content>
    <div class=breadcrumbs>
      <%= render_breadcrumbs separator: ' ＞ ' %>
    </div>
    <div id=index_header>
      <i class="fa fa-tag fa-2x"></i>
      <h1>
        ブランド一覧
      </h1>
      <div class="clearfix" id="content_header">
        <ul id="main_tab">
          <li id="tab_all" class="current"><a class="selected">すべて</a></li>
          <li id="tab_men"><a href="/ranking/all-sales-men.html">メンズ</a></li>
          <li id="tab_women"><a href="/ranking/all-sales-women.html">レディース</a></li>
          <li id="tab_kids"><a href="/ranking/all-sales-kids.html">キッズ</a>
